@{
    ViewBag.Title = "Missing Links";
}
@model MissingLinks.Models.InputModel

<p>This is where the magic happens. Maybe.</p>

@using (Html.BeginForm("SummonVeekun", "Home", FormMethod.Post))
{
    <div class="editor-label">
        @Html.LabelFor(model => model.Poke)
    </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Poke)
            @Html.ValidationMessageFor(model => model.Poke)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Move)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Move)
            @Html.ValidationMessageFor(model => model.Move)
        </div>

        <p>
            <input type="submit" value="Do the thing!" />
        </p>
}

